<section class="about-us section-wrapper">
          <h2 class="section-title section-title--alt">{{ about.title }}</h2>
          <p class="about-us__desc">{{ about.description }}</p>
          <ol class="about-us__list">
          {% for item in about.items %}
            {% include "partials/about-card.njk" %}
          {% endfor %}
          </ol>
          <!-- Read more after "About us" -->
          <div class="button-wrapper">
            {% set button = blog.sectionButton %}
            {% include "partials/button.njk" %}
          </div>
      </section>